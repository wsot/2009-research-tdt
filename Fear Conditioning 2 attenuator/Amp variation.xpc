ÿÚDim Attarr(44100)
CalibFileName = "C:\Electrophysiology Projects\Simeon Morgan\Calibrations\freefield_20090912_20cm_fft2048_15reps_10v.txt"

Set oFFS1 = CreateObject("Scripting.FileSystemObject")
Set oFFile1 = oFFS1.GetFile(CalibFileName)
Set ts1 = oFFile1.OpenAsTextStream

'%%%%%% Initialize and load calibration arrays %%%%%%%%%
count = 1

Dim tmpAttn
Do
	tmpAttn = ts1.ReadLine
	Attarr(count) = tmpAttn
	count = count + 1
Loop Until count = 44101
ts1.Close

AttF1Disp = ">AttF1"
AttF2Disp = ">AttF2"

AttSel1 = "AStim.FinalAtt1"
AttSel2 = "AStim.FinalAtt2"
Attenu1 = "Attenuator1.Atten"
Attenu2 = "Attenuator2.Atten"
FreqLvl1 = "AStim.Frequency1"
FreqLvl2 = "AStim.Frequency2"


Do While 1
	AttF1 = Read(AttSel1)
	AttF2 = Read(AttSel2)

	Freq1 = Read(FreqLvl1)
	Att1 = (Attarr(Freq1) - AttF1)
	If Att1 < 0 Then
		Write Attenu1,0
	Else
		If Freq1 = 0 Then
			Write Attenu1,120
			Write AttF1Disp,120
		Else
			If Att1 > 120 Then
				Write Attenu1,120
				Write AttF1Disp,120
			Else
				Write Attenu1,Att1
				Write AttF1Disp,Att1
			End If
		End If
	End If

	Freq2 = Read(FreqLvl2)
	Att2 = (Attarr(Freq2) - AttF2)
	If Att2 < 0 Then
		Write Attenu2,0
	Else
		If Freq2 = 0 Then
			Write Attenu2,120
			Write AttF2Disp,120
		Else
			If Att2 > 120 Then
				Write Attenu2,120
				Write AttF2Disp,120
			Else
				Write Attenu2,Att2
				Write AttF2Disp,Att2
			End If
		End If
	End If

	
'	Write Attenu,Abs(Att)
	Sleep(50)
